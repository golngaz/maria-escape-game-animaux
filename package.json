{
  "compilerOptions": {
    "target": "es6",
    "module": "es6",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "src/**/*"
  ],
  "scripts": {
    "start": "parcel index.html",
    "build": "parcel build index.html --no-minify --no-source-maps --no-cache --public-url ./ && node inject-js-inline.js"
  },
  "devDependencies": {
    "@types/howler": "^2.2.11",
    "fs": "^0.0.1-security",
    "fs-extra": "^11.2.0",
    "parcel-bundler": "^1.12.5",
    "path": "^0.12.7",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "howler": "^2.2.4"
  }
}
